   "source": [
    "N = int(input())\n",
    "\n",
    "def fibonach(f_1, f_2, cnt):\n",
    "    f_n = f_1 + f_2\n",
    "\n",
    "    if(cnt == 0):\n",
    "        return f_n\n",
    "    else:\n",
    "        return fibonach(f_2, f_n, cnt - 1)\n",
    "    \n",
    "if(N == 0):\n",
    "    print(0)\n",
    "elif(N == 1):\n",
    "    print(1)\n",
    "else:\n",
    "    print(fibonach(0, 1, N - 2))"
   ]
