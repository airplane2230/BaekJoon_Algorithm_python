    "num = int(input())\n",
    "res = 1\n",
    "\n",
    "def factorial(res, count):\n",
    "    if(num == 0):\n",
    "        return print(1)\n",
    "    else:\n",
    "        res *= count\n",
    "\n",
    "        if(count == num):\n",
    "            return print(res)\n",
    "        \n",
    "        return factorial(res, count + 1)\n",
    "factorial(res, 1)"
