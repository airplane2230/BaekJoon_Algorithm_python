    "import math\n",
    "\n",
    "num = int(input())\n",
    "\n",
    "# 분해가 전부 될때까지 loop 돌립니다.\n",
    "while num != 1:\n",
    "    for i in range(2, num + 1):\n",
    "        # 나눠지면 출력하고,\n",
    "        # 다음을 위해 해당 수로 num을 나눠줍니다.\n",
    "        if(num % i == 0):\n",
    "            print(i)\n",
    "            num = num // i\n",
    "            break"
   ]
  }
