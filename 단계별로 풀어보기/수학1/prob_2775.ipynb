    "Case = input()\n",
    "for _ in range(int(Case)):\n",
    "    k = int(input())\n",
    "    n = int(input())\n",
    "    num = [i for i in range(1, n + 1)]\n",
    "    for _ in range(k):\n",
    "        for j in range(1, n):\n",
    "            num[j] += num[j-1]\n",
    "    print(num[-1])"
