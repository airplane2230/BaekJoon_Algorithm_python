    "n, k = map(int, input().split())\n",
    "num_list = []\n",
    "for _ in range(n):\n",
    "    num_list.append(int(input()))\n",
    "    \n",
    "dp = [0] * (k+1)\n",
    "dp[0] = 1\n",
    "\n",
    "for i in num_list:\n",
    "    for j in range(i, k + 1):\n",
    "        if j - i >= 0:\n",
    "            dp[j] += dp[j - i]\n",
    "            \n",
    "print(dp[k])"
   ]
