   "source": [
    "# 브루트포스 문제는 단순 무식하게 접근한다.\n",
    "N = int(input())\n",
    "print_num = 0\n",
    "for i in range(1, N+1):\n",
    "    div_num = list(map(int, str(i)))\n",
    "    sum_num = i + sum(div_num)\n",
    "    if(sum_num == N):\n",
    "        print_num = i\n",
    "        break\n",
    "print(print_num)"
   ]
